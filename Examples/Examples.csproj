<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <Compile Update="Mancala.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Mancala.Designer.cs">
      <DependentUpon>Mancala.cs</DependentUpon>
    </Compile>
    <Compile Update="Reversi.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Reversi.Designer.cs">
      <DependentUpon>Reversi.cs</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Mancala.resx">
      <DependentUpon>Mancala.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Reversi.resx">
      <DependentUpon>Reversi.cs</DependentUpon>
    </EmbeddedResource>
  </ItemGroup>

</Project>
